package com.tocheck.parent.core.service.impl;

import com.tocheck.parent.core.dao.IToCheckSystemDao;
import com.tocheck.parent.core.entity.ToCheckSystem;
import com.tocheck.parent.core.service.ToCheckSystemService;
import org.springframework.beans.factory.annotation.Autowired;
import org.springframework.stereotype.Service;
import java.util.List;

/**
 * @author pangliang
 * @create 2017-05-22 14:49
 **/
@Service
public class ToCheckSystemServiceImpl implements ToCheckSystemService {

    @Autowired
    private IToCheckSystemDao checkSystemDao;


    @Override
    public void insert(ToCheckSystem checkSystem) {
        checkSystemDao.insert(checkSystem);
    }

    @Override
    public void logicDelete(long id) {
        checkSystemDao.logicDelete(id);
    }

    @Override
    public ToCheckSystem getById(long id) {
        return checkSystemDao.getById(id);
    }

    @Override
    public List<ToCheckSystem> getAll() {
        return checkSystemDao.getByMap(null);
    }
}

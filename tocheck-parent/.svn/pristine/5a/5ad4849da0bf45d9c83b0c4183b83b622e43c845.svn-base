package com.tocheck.parent.core.entity;

import com.tocheck.parent.common.entity.BaseEntity;

import java.math.BigDecimal;
import java.util.Date;

/**
 * @author pangliang
 * @create 2017-05-23 10:58
 **/
public class ToCheckTask extends BaseEntity {

    private Long checkId;            //检测ID
    private Long systemId;           //系统ID
    private Long userId;             //用户ID
    private Long agentId;            //代理商ID
    private String title;            //标题
    private String author;           //作者
    private int wordNum;             //字数
    private Long target;             //实际检测人
    private Long coupon;             //优惠券
    private BigDecimal money;        //付款金额
    private BigDecimal flowFee;      //流量费
    private BigDecimal targetFee;    //检测费
    private int isRush;              //是否是抢单
    private int status;              //检测状态  其他检测系统:0-正在排队，1-检测中，2-检测成功，-1检测失败,知网检测系统 0-正在排队，1-待抢单，2-正在检测，3-检测完成（已上传报告），4-已下载报告,5-用户已删除
    private int settlementStatus;    //结算状态  0-不能结算，1-可申请结算，3-检测费已申请,7-流量费已申请
    private Date createdTime;
    private Date updateTime;
    private String readUrl;          //论文下载地址
    private String downUrl;          //报告下载地址（paperfree,papertime,paperpass）
    private String errorMsg;         //提交失败原因


    public int getSettlementStatus() {
        return settlementStatus;
    }

    public void setSettlementStatus(int settlementStatus) {
        this.settlementStatus = settlementStatus;
    }

    public int getIsRush() {
        return isRush;
    }

    public void setIsRush(int isRush) {
        this.isRush = isRush;
    }

    public Date getUpdateTime() {
        return updateTime;
    }

    public void setUpdateTime(Date updateTime) {
        this.updateTime = updateTime;
    }

    public BigDecimal getTargetFee() {
        return targetFee;
    }

    public void setTargetFee(BigDecimal targetFee) {
        this.targetFee = targetFee;
    }

    public String getReadUrl() {
        return readUrl;
    }

    public void setReadUrl(String readUrl) {
        this.readUrl = readUrl;
    }

    public Long getCoupon() {
        return coupon;
    }

    public void setCoupon(Long coupon) {
        this.coupon = coupon;
    }

    public Long getTarget() {
        return target;
    }

    public void setTarget(Long target) {
        this.target = target;
    }

    public BigDecimal getMoney() {
        return money;
    }

    public void setMoney(BigDecimal money) {
        this.money = money;
    }

    public BigDecimal getFlowFee() {
        return flowFee;
    }

    public void setFlowFee(BigDecimal flowFee) {
        this.flowFee = flowFee;
    }

    public Long getUserId() {
        return userId;
    }

    public void setUserId(Long userId) {
        this.userId = userId;
    }

    public Long getAgentId() {
        return agentId;
    }

    public void setAgentId(Long agentId) {
        this.agentId = agentId;
    }

    public String getErrorMsg() {
        return errorMsg;
    }

    public void setErrorMsg(String errorMsg) {
        this.errorMsg = errorMsg;
    }

    public Long getCheckId() {
        return checkId;
    }

    public void setCheckId(Long checkId) {
        this.checkId = checkId;
    }

    public Long getSystemId() {
        return systemId;
    }

    public void setSystemId(Long systemId) {
        this.systemId = systemId;
    }

    public String getTitle() {
        return title;
    }

    public void setTitle(String title) {
        this.title = title;
    }

    public String getAuthor() {
        return author;
    }

    public void setAuthor(String author) {
        this.author = author;
    }

    public int getWordNum() {
        return wordNum;
    }

    public void setWordNum(int wordNum) {
        this.wordNum = wordNum;
    }

    public int getStatus() {
        return status;
    }

    public void setStatus(int status) {
        this.status = status;
    }

    public Date getCreatedTime() {
        return createdTime;
    }

    public void setCreatedTime(Date createdTime) {
        this.createdTime = createdTime;
    }

    public String getDownUrl() {
        return downUrl;
    }

    public void setDownUrl(String downUrl) {
        this.downUrl = downUrl;
    }
}

package com.tocheck.parent.core.properties;

import org.springframework.beans.factory.annotation.Value;
import org.springframework.stereotype.Component;

@Component
public class CommonProperties {
	// 申明实例的引用
	public static CommonProperties instance;

	public CommonProperties() {
		instance = this;
	}

	@Value("#{configProperties['paper.path']}")
	public String paper_path;

	@Value("#{configProperties['report.path']}")
	public String report_path;

	@Value("#{configProperties['plantom.path']}")
	public String plantom_path;

	@Value("#{configProperties['plantomJs.path']}")
	public String plantomJs_path;

}

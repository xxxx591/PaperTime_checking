@charset "utf-8";
/* CSS Document */
* { margin: 0; padding: 0; }
body { font-size: 14px;  font-family: Microsoft Yahei,Arial; background-color: #FFF;}
img { border: none;  }
ul, li { list-style: none; }
a { text-decoration: none;  cursor: pointer; }
input, select, button, textarea { font-size: 14px;  color: #666; outline:none;}
h1, h2, h3, h4, h5, h6, b { font-weight: normal; }
i, em { font-style: normal; }
.w1200{ width:1200px; margin:0 auto;}
.clear{ clear:both}
a:focus{outline:none;}

/*********头部***********/
.header{height: 60px; background: #3b5d98; position: fixed; left: 0; top: 0; width: 100%; z-index: 1000;}
.logo{display: block; float: left; width: 240px; text-align: center;}
.logo img{display: inline-block; margin: 17.5px auto; }
.head-nav{margin-left: 240px; height: 60px;}
.head-a{display:block; color: #fff; font-size: 16px; line-height: 60px;float: left; width: 190px; text-align: center;}
.head-as{background: #2d4979;}
.head-a:hover{background: #2d4979; color: #fff;}
.nav-left{position: fixed; left: 0; top: 0; width: 240px;  box-sizing: border-box; padding:60px 0; bottom: 0; border-right: 2px solid #3b5d98; z-index: 999;}
.nav-left-li{height: 60px;position: relative; margin-right: -2px;}
.nav-left-a{ border-bottom: 1px solid #ddd; display: block; border-right: 2px solid #3b5d98; font-size: 16px; color: #333; line-height: 60px; padding-left: 30px; box-sizing: border-box;}
.nav-left-lis .nav-left-a{border-right: 2px solid #fff; border-bottom: 1px solid #3b5d98; border-top: 1px solid #3b5d98; overflow: hidden;}
.nav-left-lis{height: 61px;}
.nav-left-a:hover{}
.nav-xsj{width: 20px; height: 20px;position: absolute; right: 20px; top: 20px; background: url("../images/icon.png") no-repeat -44px -257px; display: none;}
.nav-left-lis .nav-xsj{display: block;}
.content{margin: 60px 0 60px 240px; box-sizing: border-box; padding: 30px 120px 30px 40px;}
.footer-Submit{position: fixed; left: 0; width: 100%; bottom: 0; height: 60px; background: #f0f0f0;  color: #666666; font-size: 16px; text-align: center; padding: 17px 0; box-sizing: border-box;}
.footer-Submit p{line-height: 26px;}


/*********提交论文************/
.report-bd{position: relative; padding-right: 500px;}
.report-left{}
.report-right{position: absolute; right: 0; top: 0; width: 380px;}
.report-right-d{margin-bottom: 20px;}
.report-right-d h3{font-size: 14px; color: #333333; font-weight: bold; line-height: 26px; margin-bottom: 5px;}
.report-right-d p{font-size: 14px; color: #333;line-height: 26px;}
.layui-btn-xt{background: #e92517; font-size: 16px; color: #fff; padding: 0 26px;}
.report-left .layui-input-inline{width: auto;}
.report-one-s{line-height: 30px;font-size: 16px;}
.report-one .layui-form-label{padding: 9px 0; text-align: left; width: 65px;}
.report-one .layui-input-block{margin-left: 75px;}
.layui-input-mid{ max-width: 540px;}
.layui-input-min{max-width: 435px;}
.tjlw-tex{border: 1px solid #dbdbdb; height: 250px; width: 100%; padding: 10px; box-sizing: border-box;}
.report-one .layui-tab-content{padding: 0; margin: 20px 0;}
.report-one .layui-tab-title li{width: 50%; box-sizing: border-box; color: #333; font-size: 16px;}
.report-one .layui-tab-brief>.layui-tab-more li.layui-this:after, .layui-tab-brief>.layui-tab-title .layui-this:after{ border-color: #3b5d98; }
.report-one .layui-tab-brief>.layui-tab-title .layui-this{color: #3b5d98;font-size: 16px;}
.layui-tab-item-input .layui-form-label{ width: 120px; margin: 0;padding: 0;}
.layui-tab-item-input .layui-input-block{ margin-left: 130px;}
.layui-tab-item-input .layui-input-block .layui-input{max-width: 450px;}
.layui-tab-item-input .layui-form-item{margin: 15px 0;}
.layui-tab-item-input .layui-form-item p{line-height: 26px; font-size: 14px; color: #666;}
.layui-btn-next{background: #3b5d98; height: 50px; line-height: 50px; font-size: 16px; color: #fff; min-width: 140px;}
.layui-input-inline-right{float: right;}
.layui-input-inline-right span{font-size: 16px; color: #333;}
.layui-input-inline-right em{ font-size: 14px; color: #666;}
.layui-input-inline-right i{display: inline-block; vertical-align: middle; width: 16px; height: 16px; border-radius: 16px; margin: 0 2px;}
.layui-input-inline-right .ico-one{background: #b9c3d5;}
.layui-input-inline-right .ico-two{background: #f49243;}
.layui-input-inline-right .ico-three{background: #b9c3d5;}
.report-bd .layui-form-item{margin-bottom: 30px;}
.report-bd .report-two .layui-form-item{margin: 0;}
.report-two .report-two-top .layui-form-label span{display: inline-block;border-right: 1px solid #ddd; padding-right:15px;font-size: 16px; }
.report-two .report-two-top .layui-input-block .layui-input{border: none; border-bottom: 1px solid #ddd;}
.price{line-height: 38px; color: #666;    padding-left: 12px;}
.price strong{font-size: 16px; font-weight: bold; color: #e92517; margin-right: 5px;}
.report-two-top{border: 1px solid #ddd;}
.report-two-top .layui-form-label{line-height: 31px;}
.report-two .report-two-top .layui-input-block{line-height: 48px;}
.report-two .report-two-top .layui-input-block .layui-input{height: 50px; line-height: 48px;}
.report-two-bd{margin: 30px 0 20px;}
.report-two-bd h3{font-size: 16px; color: #333; line-height:26px;}
.report-two-bd hr{ background: #3b5d98; height: 2px;}
.report-two-bd .layui-tab-title{border: none;}
.report-two-bd .layui-tab-title li{width: 160px; height: 40px; margin-right: 20px; background: url("../images/icon.png") no-repeat -429px -326px; padding: 0;}
.report-two-bd .layui-tab-title .layui-this{background-position:-429px -256px;}
.report-two-bd .layui-tab-title .layui-this:after{border: none;}
.left-tb{width: 435px; float: left;}
.right-tb{margin-left: 495px; padding-top: 30px;}
.report-two-bd .layui-tab{ margin: 30px 0;}
.report-two-bd .layui-tab .layui-tab-title{margin-bottom:45px;}
.report-two-t{color: #4c4c4c; font-size: 16px; line-height: 26px; margin-bottom:25px;}
.report-two .report-two-bd .layui-form-label{padding: 9px 0;}
.report-two .report-two-bd .layui-bt{margin-bottom: 20px;}
.right-tb .layui-btn{margin-right: 15px; background: #ee7000; width: 140px;}
.right-tb span{font-size: 14px; color: #333;}
.sm-left{float: left;  width: 280px; margin-left: 200px;    padding-top: 40px;}
.sm-right{ margin-left: 520px;}
.sm-left strong{color: #333333; font-size: 16px;}
.sm-left .layui-input{margin: 20px 0;}
.sm-left p{font-size: 16px; color: #e92517;}
.sm-ewm{width: 180px; height: 180px;    margin: 0 5px}
.sm-ewm img{display: block; width: 100%; height: 100%; margin: 0 auto;}
.sm-right i{display: inline-block; vertical-align: middle; width: 25px; height: 25px; margin: 0 3px; background: url("../images/icon.png") no-repeat -636px -256px;}
.sm-right p{font-size: 16px;color: #333; line-height: 26px; margin-top: 5px; width: 192px; text-align: center;}
.report-three-bt .layui-btn{ width: 120px; height: 40px;background: #49a3c0; color: #fff;font-size: 16px; }
.report-three{width:260px; margin: 90px auto 30px; text-align: center;}
.report-three-xs{display: inline-block; vertical-align: middle; margin-bottom: 10px; background: url("../images/icon.png") no-repeat -118px -257px; width: 60px; height: 60px;}
.report-three .report-three-d{ text-align:left;}
.report-three-d{margin:25px 0; padding-left: 55px; box-sizing: border-box;}
.report-three-d strong{font-size: 18px; color: #333; font-weight: bold; line-height: 26px; margin-bottom: 10px; display: block;}
.report-three-d p{font-size: 16px; color: #333; line-height: 26px; margin-bottom: 10px;}
.report-three-d p span{color: #666;}
.report-three-d p em{color: #3b5d98;}
.report-three-cg{color: #3b5d98; font-size: 16px;}

















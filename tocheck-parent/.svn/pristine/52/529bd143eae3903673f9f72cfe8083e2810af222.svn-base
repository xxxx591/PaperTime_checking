<%@ page contentType="text/html;charset=UTF-8" language="java" %>
<!DOCTYPE html>
<html>
<head>
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <title>登录</title>
    <link href="/resources/style/bootstrap.css" rel="stylesheet">
    <link rel="stylesheet" href="/resources/style/font-awesome.css">
    <link href="/resources/style/style.css" rel="stylesheet">
    <!-- HTML5 Support for IE -->
    <script src="/resources/js/html5shim.js"></script>
    <script src="/resources/js/jquery.js"></script>
</head>
<body>

<!-- Form area -->
<div class="admin-form">
    <div class="container">
        <div class="row">
            <div class="col-md-12">
                <!-- Widget starts -->
                <div class="widget worange" style="margin-top: 200px">
                    <!-- Widget head -->
                    <div class="widget-head">
                        <i class="icon-lock"></i> tocheck运营后台管理系统
                    </div>
                    <div class="widget-content">
                        <div class="padd">
                            <!-- Login form -->
                            <form class="form-horizontal" method="post">
                                <!-- Email -->
                                <div class="form-group">
                                    <label class="control-label col-lg-3">账号</label>
                                    <div class="col-lg-9">
                                        <input type="text" class="form-control" name="userName">
                                    </div>
                                </div>
                                <!-- Password -->
                                <div class="form-group">
                                    <label class="control-label col-lg-3">密码</label>
                                    <div class="col-lg-9">
                                        <input type="password" class="form-control" name="password">
                                    </div>
                                </div>
                                <p class="login-li-ts-p" style="display:none;padding-left:90px;">
                                    <i class="login-li-ts-i">×</i>
                                    <span class="login-li-s">请填写正确的手机号</span>
                                </p>
                                <div class="col-lg-9 col-lg-offset-2">
                                    <button type="button" class="btn btn-danger" id="admin-login">管理员登录</button>
                                    <button type="button" class="btn btn-default" id="agent-login">代理商登录</button>
                                </div>
                                <br/>
                            </form>
                        </div>
                    </div>
                    <div class="widget-foot">
                        没有账号? <a href="/register.html">代理商注册</a>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
<!-- JS -->
<script src="/resources/js/bootstrap.js"></script>
<script src="/resources/js/commons.js"></script>
<script src="/resources/js/layer/layer.js"></script>
<script src="/resources/js/register.js"></script>
</body>
</html>

function selectAll() {
    $("tbody").find("input[type='checkbox']").prop("checked", true);
}
function cancelAll() {
    $("tbody").find("input[type='checkbox']").each(function () {
        if ($(this).is(':checked')) {
            $(this).prop("checked", false);
        } else {
            $(this).prop("checked", true);
        }
    })
}
function deletes() {
    var valArr = new Array;
    $("tbody").find("input[type='checkbox']:checked").each(function (i) {
        valArr[i] = $(this).val();
    })
    var ids = valArr.join(',');//转换为逗号隔开的字符串
    if (valArr == "") {
        return;
    }
    del(ids);
}

function del(ids) {
    layer.confirm("您确定要删除吗?", {
        title:['系统提示' , true],
        btn: ['确定', '取消'] //按钮
    }, function () {
        var index= loading();
        $.post('deleteReport.html', {ids: ids}, function (json) {
            layer.close(index);
            if (json.state) {
                location.reload();
            } else {
                layer.msg(json.message);
            }
        }, 'json')
    }, function () {
        layer.close();
    });

}
.news-right-top{ height: 40px; font-size: 14px; font-weight: bold; color: #333; line-height: 40px; border-top: 2px solid #49a3c0; border-bottom: 1px solid #49a3c0;}
.news-right-list{margin: 20px 0;}
.news-right-a{display: block; line-height: 26px; margin-bottom: 15px;}
.news-right-a i{display: block; float: left; width: 18px; height: 18px; border-radius: 2px; text-align: center; line-height: 18px; font-size: 12px; color: #fff; margin-right: 5px; background: #bfbfbf; margin-top: 5px;}
.news-right-a:nth-of-type(1) i{background: #fe764c;}
.news-right-a:nth-of-type(2) i{background: #fda029;}
.news-right-a:nth-of-type(3) i{background: #fdc929;}

.news-nr{background: #fff; border: 1px solid #e6e6e6; padding:40px 20px; box-sizing: border-box;}
.news-nr .news-left-nr{width: 800px;}
.news-left-nr h3{ text-align: center; font-size: 16px; color: #333; margin-bottom: 10px;}
.news-left-nr h4{text-align: center; font-size: 14px; line-height: 26px; color:#666;}
.news-left-nr h4 span{ margin: 0 10px;}
.news-left-nr p{line-height: 24px; color: #666; font-size: 14px;  text-indent: 2em; margin-bottom: 10px;}
.nr-next{margin: 20px 0;}
.nr-next a{display: block; color: #4c4c4c; line-height:28px; font-size: 14px; width: 600px; white-space: nowrap; overflow: hidden; height: 28px;}
.nr-next .nr-next-a{font-weight: bold;}
#demo2{text-align: center;}

.upload-file-box{position:relative; border: 1px solid #ddd; width: 120px; height: 38px; display: block; line-height: 38px; color: #333; padding-left: 10px; box-sizing: border-box; cursor: pointer;}
.upload-file-box i{display: inline-block; vertical-align: middle;width: 28px; height: 28px; background:url("../images/icon.png") -720px -258px no-repeat; margin-right: 3px; cursor: pointer;}
.upload-file-box input{position: absolute; left: 0; top: 0; opacity: 0; display: block; width: 120px; height: 38px; cursor: pointer;}












































